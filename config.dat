# =================================================================
# OBS C SDK Benchmark Configuration File
# =================================================================

# -----------------------------------------------------------------
# [Environment]
# -----------------------------------------------------------------
Endpoint=obs.ap-southeast-1.myhuaweicloud.com
Protocol=https
KeepAlive=true

# -----------------------------------------------------------------
# [Security & GM Configuration]
# -----------------------------------------------------------------
GmModeSwitch=false
SslMinVersion=1.2
SslMaxVersion=1.3
MutualSslSwitch=false
ClientCertPath=
ClientKeyPath=
ClientKeyPassword=

# -----------------------------------------------------------------
# [Multi-User Concurrency Settings]
# -----------------------------------------------------------------
Users=1
ThreadsPerUser=1
BucketNamePrefix=bench.test
BucketNameFixed=

# -----------------------------------------------------------------
# [Test Plan]
# -----------------------------------------------------------------
TestCase=201
RunSeconds=60
RequestsPerThread=1

# [Mixed Mode Settings] (Only used if TestCase=900)
MixOperation=201,202,204
MixLoopCount=100

# -----------------------------------------------------------------
# [Object Configuration]
# -----------------------------------------------------------------
ObjectSize=5242880
PartSize=5242880
KeyPrefix=obj
ObjNamePatternHash=true

# -----------------------------------------------------------------
# [Advanced: Data Validation]
# -----------------------------------------------------------------
# 数据一致性校验 (true/false)
# 开启后，下载时会进行全量内存数据比对 (In-Memory/File Isomorphic Verification)
EnableDataValidation=true

# -----------------------------------------------------------------
# [Resumable / Multipart Source]
# -----------------------------------------------------------------
# 上传文件路径。
# 如果该文件不存在，工具会自动使用符合数据一致性检验的算法生成确定性内容的文件。
UploadFilePath=./test_data.bin
EnableCheckpoint=true

# -----------------------------------------------------------------
# [Logging]
# -----------------------------------------------------------------
LogLevel=INFO
